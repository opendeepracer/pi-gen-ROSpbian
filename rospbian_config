# Generate a ros PI image
IMG_NAME="ROSpbian"
LOCALE_DEFAULT="en_GB.UTF-8"
TARGET_HOSTNAME="rospberry"
KEYBOARD_KEYMAP="gb"
KEYBOARD_LAYOUT="English (US)"
TIMEZONE_DEFAULT="Etc/UTC"
FIRST_USER_NAME="ros"
FIRST_USER_PASS="melodic"
ENABLE_SSH=1
STAGE_LIST="stage0 stage1 stage2 stage2-ros stage3 stage4"
PI_GEN="pi-gen-rospbian"
PI_GEN_REPO="https://github.com/opendeepracer/pi-gen-ROSpbian"
